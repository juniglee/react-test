doctype html
html.static.detail-full-width-block-index-page.contents.js.flexbox.rgba.hsla.multiplebgs.backgroundsize.borderimage.borderradius.boxshadow.textshadow.opacity.cssanimations.csscolumns.csstransitions.fontface.generatedcontent
    block variables
    head
        meta(charset='utf-8')
        meta(content='IE=edge,chrome=1', http-equiv='X-UA-Compatible')
        meta(content='text/html; charset=utf-8' http-equiv='content-type')
        meta(content="index,follow" name="ROBOTS")
        meta(content="width=device-width, initial-scale=1" name="viewport")
        meta(content=description name="description")
        title World Nomads - #{title}

        block layout_css

        link(href="https://cdn.worldnomads.net/Themes/Nomads/Styles/cms_screen.wn.min.css" rel="stylesheet" type="text/css")
        link(rel="stylesheet" type="text/css" href="/css/wn-cms-styles-dd-mm-yy.min.css")
        link(href="/Themes/Nomads/Images/favicon.png" rel="icon")

        <!--[if !IE8]>
        script(type="text/javascript" async="" src="//cdn.segment.com/analytics.js/v1/NK6eO63SbmKTx3i4RJq39BjEtLzquoKW/analytics.min.js")
        <![endif]-->

        script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js" type="text/javascript")
        script(src="https://cdn.worldnomads.net/Scripts/jquery/autocomplete/jquery.autocomplete.js")
        script(src="https://cdn.worldnomads.net/Scripts/jquery/datepicker/jquery.datePicker.js")
        script(src="https://cdn.worldnomads.net/Scripts/jquery/datepicker/jquery.datePickerForSelects.js")
        script(src="/Themes/Nomads/scripts/modernizr.js" type="text/javascript")
    body(class="cms borderless" data-feedly-mini="yes")
        .content
            .full-width-block-index-page#layout-wrapper
                include header.pug
                .zone.zone-content
                    // Content begins here
                    block body
                    // Content ends here
                    script(src="/assets/js/foundation.min.js")
                    // Include Foundation top-bar script
                    script(src="/assets/js/foundation.topbar.min.js")
                    script.
                        $(document).foundation();
                include footer.pug

            block scripts
            script(src="/Themes/Nomads/scripts/fastclick.js" type="text/javascript")
            script(src="/Themes/Nomads/scripts/tooltip.js" type="text/javascript")
            script(src="/Themes/Nomads/scripts/popover.js" type="text/javascript")
            script(src="/Themes/Nomads/scripts/spin.js" type="text/javascript")
            script(src="/Themes/Nomads/scripts/wng.animations.js" type="text/javascript")
            // script(src="/Themes/Nomads/scripts/wng.responsive-cms.js" type="text/javascript")
        <!--[if lte IE9]>
        script(type="text/javascript" src="/Themes/Nomads/scripts/respond.min.js")
        <![endif]-->
